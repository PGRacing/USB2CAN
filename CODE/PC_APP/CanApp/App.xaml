<Application x:Class="CanApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CanApp"
             xmlns:converters="clr-namespace:CanApp.Converters" 
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <converters:ByteArrayToStringConverter x:Key="ByteArrayToStringConverter"/>
            <converters:ByteIndexEnabledConverter x:Key="ByteIndexEnabledConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
